# Flask Settings
FLASK_ENV=development
FLASK_APP=app_enhanced.py
SESSION_SECRET=your_session_secret_here
JWT_SECRET_KEY=your_jwt_secret_here

# Database Configuration
DATABASE_URL=sqlite:///roboto_sai_complete.db
SQLALCHEMY_TRACK_MODIFICATIONS=False

# API Keys (Replace with your own keys)
OPENAI_API_KEY=your_openai_api_key_here
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=your_pinecone_index_name_here

# X/Twitter API Configuration
X_API_TOKEN=your_x_api_token_here
X_API_SECRET=your_x_api_secret_here
XAI_API_KEY=your_xai_api_key_here

# Roboto API Configuration
ROBOTO_API_KEY=your_roboto_api_key_here

# Replit Integration
REPL_ID=your_repl_id_here

# GitHub App Configuration (for GitHub App deployment)
GITHUB_APP_ID=your_github_app_id
GITHUB_PRIVATE_KEY_PATH=/path/to/private-key.pem
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# AWS Configuration (optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Quantum Configuration
QUANTUM_AVAILABLE=true
QISKIT_AVAILABLE=true

# Voice Processing
SPEECH_RECOGNITION_AVAILABLE=true
PYGAME_AVAILABLE=true

# Development Settings
DEBUG=true
LOG_LEVEL=INFO